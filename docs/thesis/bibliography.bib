  title = {State Variation in Effects of State Social Distancing Policies on {{COVID-19}} Cases},
  author = {Kaufman, Brystana G. and Whitaker, Rebecca and Mahendraratnam, Nirosha and Hurewitz, Sophie and Yi, Jeremy and Smith, Valerie A. and McClellan, Mark},
